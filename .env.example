PRIVATE_KEY=your_private_key
RPC_URL=https://rpc.fullnode.kuru.io/
RPC_WS_URL=wss://rpc.fullnode.kuru.io/
MARKET_ADDRESS=0x065C9d28E428A0db40191a54d33d5b7c71a9C394

# Logging
SDK_LOG_LEVEL=INFO          # SDK log verbosity: DEBUG, INFO, WARNING, ERROR (default: INFO)

# Position Management
MAX_POSITION=1000
OVERRIDE_START_POSITION=     # Optional: manual override (leave blank for auto-detect from margin)
RECONCILE_INTERVAL=300       # Seconds between position reconciliation (0=disabled)

# Strategy Parameters
PROP_SKEW_ENTRY=0.5
PROP_SKEW_EXIT=0.5
STRATEGY_TYPE=long

# Order Sizing (choose one method)
# Method 1: Fixed quantity per level (legacy)
QUANTITY=100

# Method 2: Quantity as BPS of max position (recommended - overrides QUANTITY if set)
QUANTITY_BPS_PER_LEVEL=500   # 500 = 5% of max position per quoter level

# Quoter Configuration
QUOTERS_BPS=25,50,75
PROP_MAINTAIN=0.2  # Cancel threshold: orders kept if edge >= (1-0.2)*target_edge
