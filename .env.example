# ========================================
# REQUIRED SECRETS
# ========================================

PRIVATE_KEY=your-private-key

# Required unless strategy.market_address is set in bot_config.toml
MARKET_ADDRESS=0x0000000000000000000000000000000000000000

# ========================================
# LOGGING
# ========================================

# Bot log level for SDK + app logs routed through loguru
SDK_LOG_LEVEL=INFO

# ========================================
# SDK CONNECTION ENDPOINTS (optional)
# ========================================

# Defaults (SDK):
# RPC_URL=https://rpc.monad.xyz
# RPC_WS_URL=wss://rpc.monad.xyz
# KURU_WS_URL=wss://ws.kuru.io/
# KURU_API_URL=https://api.kuru.io/

RPC_URL=
RPC_WS_URL=
KURU_WS_URL=
KURU_API_URL=

# ========================================
# SDK EXECUTION / TX SETTINGS (optional)
# ========================================

# Subscription type for RPC logs stream (default: monadLogs)
KURU_RPC_LOGS_SUBSCRIPTION=monadLogs

# Gas estimate multipliers/buffers
KURU_GAS_BUFFER_MULTIPLIER=1.3
# KURU_GAS_BUFFER=80000

# Order execution defaults
KURU_POST_ONLY=false
KURU_USE_ACCESS_LIST=true

# ========================================
# SDK WEBSOCKET / RECOVERY SETTINGS (optional)
# ========================================

# RPC websocket reconnect tuning
# KURU_RPC_WS_MAX_RECONNECT_ATTEMPTS=0
# KURU_RPC_WS_RECONNECT_DELAY=1.0
# KURU_RPC_WS_MAX_RECONNECT_DELAY=30.0

# Reconciliation loop tuning for stuck ORDER_SENT recovery
# KURU_RECONCILIATION_INTERVAL=3.0
# KURU_RECONCILIATION_THRESHOLD=5.0

# ========================================
# OPERATIONAL STRATEGY SETTINGS
# ========================================
# Operational strategy settings are in bot_config.toml.
# See bot_config.example.toml for supported fields.
